{
  "version": 2,
  "name": "mascgram-app",
  "builds": [
    {
      "use": "@vercel/static-build",
      "src": "packge.json"
    }
  ],
  "routes": [
    {
      "src": "(.*).js",
      "dest": "$1.js"
    },
    {
      "src": "(.*).json",
      "dest": "$1.json"
    },
    {
      "src": "/.*", "dest": "/index.html"
    }
  ]
}